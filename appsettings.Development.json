{
  "DatabaseProvider": "DroHubConnectionMySQL",
  "AutoMigration": true,
  "RPCAPIVersion": 1.0,
  "ConnectionStrings": {
    "DroHubConnectionMySQL": "Server=localhost;port=3306;database=master"
  },
  "SubscriptionInfo" : {
    "FREE": {
      "AllowedFlightTimeInMinutes": 10,
      "AllowedUserCount": 2,
      "AllowedVotes": 0,
      "Price": 0
    },
    "STANDARD": {
      "AllowedFlightTimeInMinutes": 60,
      "AllowedUserCount": 4,
      "AllowedVotes": 2,
      "Price": 29
    },
    "PRO": {
      "AllowedFlightTimeInMinutes": 120,
      "AllowedUserCount": 8,
      "AllowedVotes": 4,
      "Price": 64
    }
  },
  "KeysDirectory": "/tmp/",
  "MailJetEmailSender": {
    "APIKey": "@@ENV@@",
    "APISecret": "@@ENV@@",
    "FromEmailAddress": "postmaster@drohub.xyz"
  },
  "RepositoriesConfiguration": {
    "GoogleMapsAPIKey": "@@ENV@@",
    "FrontEndStunServerUrl": "stun:stun.l.google.com:19302",
    "FrontEndJanusUrl": "http://localhost:8088/janus"
  },
  "JanusServiceOptions" : {
    "Address" : "http://localhost",
    "Port": 8088,
    "AdminKey": "@@ENV@@"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override" : {
          "Microsoft" : "Warning",
          "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=localhost;Database=master;MultipleActiveResultSets=true;User Id=sa;Password=#ABc123456789;",
          "tableName": "Logs",
          "autoCreateSqlTable": false,
          "restrictedToMinimumLevel" : "Information"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel" : "Debug",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message,30:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "SQLite",
        "Args": {
          "sqliteDbPath": "/home/pneves/Projects/drohub/DroHub/drohub.db",
          "tableName": "Logs",
          "storeTimestampInUtc": true
        }
      }
    ]
  }
}