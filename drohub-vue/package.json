{
  "name": "drohub-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prepare": "patch-package",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@algoz098/vue-player": "https://github.com/algoz098/vue-player.git#8c2a2fcca2312357836650a16f9f9b5da953c8e5",
    "@aspnet/signalr": "^1.1.4",
    "@hydrant/eslint-config": "^2.1.3",
    "@johmun/vue-tags-input": "^2.1.0",
    "@popperjs/core": "^2.4.4",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "email-validator": "^2.0.4",
    "qs": "^6.9.4",
    "v-click-outside": "^3.0.1",
    "vue": "^2.6.11",
    "vue-inline-svg": "^1.3.1",
    "vue-slider-component": "^3.2.5",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "patch-package": "^6.2.2",
    "postinstall-prepare": "^1.0.1",
    "sass-loader": "^9.0.0",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11",
    "vue-waypoint": "^3.4.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "json",
      "js",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)(\\?inline)?$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "^(test-data.json)$": "<rootDir>/tests/$1",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/tests/jest.overrides.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@algoz098)"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "no-underscore-dangle": "off",
      "camelcase": "off",
      "brace-style": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
