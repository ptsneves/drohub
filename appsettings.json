{
  "DatabaseProvider": "mysql",
  "AutoMigration": true,
  "ConnectionStrings": {
    "DroHubConnectionMSSQL": "Server=localhost;Database=master;MultipleActiveResultSets=true;User Id=sa;Password=#ABc123456789",
    "DroHubConnectionMySQL": "Server=localhost;port=3306;database=master"
  },
  "SubscriptionInfo" : {
    "BETA_TEST": {
      "AllowedFlightTimeInMinutes": 120,
      "AllowedUserCount": 2
    },
    "STANDARD": {
      "AllowedFlightTimeInMinutes": 60,
      "AllowedUserCount": 4
    },
    "PRO": {
      "AllowedFlightTimeInMinutes": 120,
      "AllowedUserCount": 8
    }
  },
  "KeysDirectory": "/opt/keys/",
  "MailJetEmailSender": {
    "APIKey": "@@ENV@@",
    "APISecret": "@@ENV@@",
    "FromEmailAddress": "postmaster@drohub.xyz"
  },
  "AllowedHosts": "*",
  "RepositoriesConfiguration": {
    "GoogleMapsAPIKey": "@@ENV@@",
    "FrontEndStunServerUrl": "stun:stun.l.google.com:19302",
    "FrontEndJanusUrl": "/janus"
  },
  "JanusServiceOptions": {
    "Address": "http://localhost",
    "Port": 8088,
    "AdminKey": "@@ENV@@"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override" : {
          "Microsoft" : "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel" : "Information",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message,30:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  }
}
