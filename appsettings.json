{
  "DatabaseProvider": "sqlite3",
  "ConnectionStrings": {
    "DroHubConnectionMSSQL": "Server=localhost;Database=master;MultipleActiveResultSets=true;User Id=sa;Password=#ABc123456789"
  },
  "AllowedHosts": "*",
  "RepositoriesConfiguration": {
    "Dropbox": {
      "APIKey": "@@ENV@@",
      "APISecret": "@@ENV@@",
      "AppName": "DroHub",
      "AuthRedirectUri": "https://drohub.airborneprojects.com/DHub/DeviceRepository/Auth"
    },
    "GoogleMapsAPIKey": "@@ENV@@",
    "FrontEndStunServerUrl": "stun:stun.l.google.com:19302",
    "FrontEndJanusUrl": "/janus"
  },
  "JanusServiceOptions": {
    "Address": "http://localhost",
    "Port": 8088,
    "AdminKey": "@@ENV@@",
    "RTPPortStart": 6004,
    "RTPPortEnd": 7004
  },
  "DeviceMicroServiceOptions": {
    "Address": "drone",
    "Port": 50051
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override" : {
          "Microsoft" : "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel" : "Debug",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message,30:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  }
}